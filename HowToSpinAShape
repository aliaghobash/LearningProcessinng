//float x;
//float y;
float angle;
void setup() {
  size(400, 400);
  
};
void draw() {
  background(0);
  fill(255);
  translate(200,200);
  //rectMode(CENTER);
  ellipseMode(CENTER);
  //x=x+1;
  //y=y+1;
  rotate(angle);
  //rect(0, 0, 100, 50);
  ellipse(0, 0, 100, 100);
  angle=angle+1*0.1;
};
